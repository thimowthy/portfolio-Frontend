{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "warn",
    "object-curly-spacing": ["warn", "always"],
    "no-multi-spaces": "warn",
    "array-bracket-spacing": ["warn", "always"],
    "no-await-in-loop": "warn"
  }
}
